{
  "expo": {
    "name": "FirebaseGoogleAuthApp",
    "slug": "FirebaseGoogleAuthApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "firebasegoogleauthapp",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/logo-f.png",
      "resizeMode": "contain",
      "backgroundColor": "#EBF8FF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
